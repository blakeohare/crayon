/*
    args[0] -> reference to the GameWindow object
*/
Value lib_game_setInstance(Array<Value> args) {
    ObjectInstance o = (ObjectInstance)args[0].internalValue;
    Array<object> nd = new Array<object>(1);
    nd[0] = Native._lib_game_getNativeWindowHandle();
    o.nativeData = nd;
    return VALUE_NULL;
}
