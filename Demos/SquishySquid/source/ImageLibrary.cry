import Graphics2D;

static class ImageLibrary {
    static field images = {};

    static function get(path) {
        return ImageLibrary.images.get(path);
    }

    static function populateLibrary(images) {
        for (file : images.keys()) {
            key = file['images/'.length:];
            ImageLibrary.images[key] = images[file];
        }
    }
}
