/*
    args[0] -> (ObjectInstance) native instance (instantiated)
    args[1] -> (int) property key
    args[2] -> (Value<various>) property value
*/
VM.Value lib_nori_invalidateWindowProperty(VM.VmContext vm, Array<VM.Value> args) {
    VM.ObjectInstance o = (VM.ObjectInstance) args[0].internalValue;
    int key = (int) args[1].internalValue;
    object value = args[2].internalValue;
    Extension._lib_nori_invalidateWindowProperty(o.nativeData[0], key, value);
    return vm.globalNull;
}
