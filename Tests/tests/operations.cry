function test() {
	a = 37;
	b = 42;
	
	x = -1;
	y = 2;
	
	print(a + b);
	print(a - b);
	print(a * b);
	print(a / b);
	print(a % b);
	print(x + y);
	print(x - y);
	print(x * y);
	print(x / y);
	print(x % y);
	print(x ** y);
	
	a += 0.0;
	y += 0.0;
	
	print(a + b);
	print(a - b);
	print(a * b);
	print((a / b + '')[:10]);
	print(a % b);
	print(x + y);
	print(x - y);
	print(x * y);
	print(x / y);
	print(x % y);
	print(x ** y);
	
	powerTest();
}


function powHelper(baseNum, powerNum) {
	try {
		return [baseNum, powerNum, baseNum ** powerNum];
	} catch (InvalidArgumentException iae) {
		return [baseNum, powerNum, iae.message];
	}
}

function powerTest() {
	print('exponents');
	
	bases = [-2, 2, 16, 16.0, 1.5, 1, .5, 0, 0.0, -.5, -1, -1.0, -3, -3.0];
	powers = [5, 2.5, 1, .5, 0, 0.0, -.5, -1, -2];
	
	for (b : bases) {
		for (p : powers) {
			emit(powHelper(b, p));
		}
	}
	
}