function test() {
	print('basics');
	a = [1, 2, 3, 4];
	emit(a);
	a.clear();
	emit(a);
	a.add(5);
	emit(a);
	a.add(6, 7);
	emit(a);
	a.reverse();
	emit(a);
	a.pop();
	emit(a);
	
	print('concats');
	b = [5, 4, 3];
	emit(a + b);
	emit(a);
	emit(b);
	a.concat(b);
	emit(a);
	emit(b);
	c = a.clone();
	c.add(2);
	emit(c);
	emit(a);
	
	print('contains');
	emit(a.contains(4));
	emit(a.contains(99));
	
	print('inserts');
	a.insert(0, 7);
	a.insert(0, 8);
	a.insert(0, 9);
	a.insert(-1, 10);
	emit(a);
	
	print('join');
	emit(a.join(':'));
	
	print('shuffle');
	shuffleTest = [];
	for (i = 1; i <= 100; ++i) {
		shuffleTest.add(i);
	}
	shuffleTest.shuffle();
	slice = shuffleTest[:10];
	sum = 0;
	for (n : slice) {
		sum += n;
	}
	emit(sum != 55); // statistical impossibility with 100 numbers choosing the first 10.
	
	print('sort');
	shuffleTest.sort();
	emit(shuffleTest[::10]);
	
	shuffleTest.shuffle();
	shuffleTest.sort((num) => { (num + '').reverse(); });
	emit(shuffleTest[:20]);
}
