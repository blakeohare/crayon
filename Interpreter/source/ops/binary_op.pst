    case (((Types.FLOAT * MAX_OP) + BinaryOps.SUBTRACTION) * MAX_TYPE) + Types.FLOAT:
        Core.EmitComment("float - float");
        float1 = (double) leftValue.internalValue - (double) rightValue.internalValue;
