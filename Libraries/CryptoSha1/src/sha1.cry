import CryptoCommon;

namespace CryptoSha1 {

    class Sha1Hash : CryptoCommon.AbstractHash {

        constructor(disableWarning = null) : base() {
            this.showWarning('SHA1', disableWarning);
        }

        function digestImpl() {
            return $$$('cryptoDigest', this._bytes, 2);
        }
    }
}
