/*
    args[0] -> (ObjectInstance) child - already instantiated, but not in UI tree
    args[1] -> (ObjectInstance) parent - already instantiated
*/
VM.Value lib_nori_addChildToParent(VM.VmContext vm, Array<VM.Value> args) {
    VM.ObjectInstance child = (VM.ObjectInstance) args[0].internalValue;
    VM.ObjectInstance parent = (VM.ObjectInstance) args[1].internalValue;
    Extension._lib_nori_addChildToParent(child.nativeData[0], parent.nativeData[0]);
    return vm.globalNull;
}
