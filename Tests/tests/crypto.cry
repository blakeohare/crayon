import CryptoMd5;
import CryptoSha1;

function md5(text) {
	m = new Md5Hash('unsafe');
	m.addText(text);
	return m.digestHex();
}

function sha1(text) {
	s = new Sha1Hash('unsafe');
	s.addText(text);
	return s.digestHex();
}

function test() {
	print('md5');
	print(md5(''));
	print(md5('abc'));
	print(md5('cheese'));
	print(md5('The quick brown fox jumped over the lazy dogs'));
	print(md5('z' * 1000));
	
	ignored = new Md5Hash(); // generate warning
	print(new Md5Hash('unsafe').addText('abc').digestByteList().join(' '));
	print(new Md5Hash('unsafe').addByte(97).addByte(98).addByte(99).digestHex());
	
	print('---');
	
	print('sha1');
	print(sha1(''));
	print(sha1('abc'));
	print(sha1('cheese'));
	print(sha1('The quick brown fox jumped over the lazy dogs'));
	print(sha1('z' * 1000));
	
	ignored = new Sha1Hash(); // generate warning
	print(new Sha1Hash('unsafe').addText('abc').digestByteList().join(' '));
	print(new Sha1Hash('unsafe').addByte(97).addByte(98).addByte(99).digestHex());
	
}
