/*
	args[0] -> Frame object
	args[1] -> String of UI commands, comma separated.
*/
VM.Value lib_norialpha_showFrame(VM.VmContext vm, Array<VM.Value> args) {
	VM.ObjectInstance frameObj = (VM.ObjectInstance)args[0].internalValue;
	string data = lib_norialpha_encodeListToWireFormat(args[1]);
	frameObj.nativeData = new Array<object>(1);
	frameObj.nativeData[0] = Extension._lib_norialpha_showFrame(data);
	return vm.globalNull;
}
