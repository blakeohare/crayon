/*
    Returns the user's home directory.
    - Windows: "C:\Users\Cthulhu"
    - Linux: "~"
    - JavaScript: "/" (this is used for the virtualized disk)
*/
VM.Value lib_fileiocommon_getUserDirectory(VM.VmContext vm, Array<VM.Value> args) {
    return VM.buildString(vm.globals, Native._lib_fileiocommon_getUserDirectory());
}
