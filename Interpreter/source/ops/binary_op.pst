leftValue = valueStack[valueStackSize - 1];

switch ((leftValue.type * MAX_OP + row[0]) * MAX_TYPE + rightValue.type) {

    case (Types.INTEGER * MAX_OP + BinaryOps.EXPONENT) * MAX_TYPE + Types.INTEGER:
