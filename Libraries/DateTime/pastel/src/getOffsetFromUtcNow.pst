/*
    args[0] -> (ObjectInstance) nullable time zone
        nativeData[0] -> native time zone data

*/
VM.Value lib_datetime_getOffsetFromUtcNow(VM.VmContext vm, Array<VM.Value> args) {
    object nativeTz = lib_datetime_getNativeTimezone(args[0]);

    if (nativeTz == null) return vm.globals.intZero;

    int offset = Extension._lib_datetime_getOffsetFromUtcNow(nativeTz);
    return VM.buildInteger(vm.globals, offset);
}
