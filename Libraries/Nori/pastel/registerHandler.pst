/*
    args[0] -> (ObjectInstance) element
    args[1] -> (string) type
    args[2] -> (int) handler ID#
*/
VM.Value lib_nori_registerHandler(VM.VmContext vm, Array<VM.Value> args) {
    VM.ObjectInstance element = (VM.ObjectInstance) args[0].internalValue;
    Extension._lib_nori_registerHandler(
        element.nativeData[0],
        (int) args[1].internalValue,
        (string) args[2].internalValue,
        (int) args[3].internalValue);
    return vm.globalNull;
}
