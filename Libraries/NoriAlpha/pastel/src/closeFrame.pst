/*
	args[0] -> Frame object
*/
VM.Value lib_norialpha_closeFrame(VM.VmContext vm, Array<VM.Value> args) {
    VM.ObjectInstance frameObj = (VM.ObjectInstance)args[0].internalValue;
	object nativeFrameHandle = frameObj.nativeData[0];
	Extension._lib_norialpha_closeFrame(nativeFrameHandle);
    return vm.globalNull;
}
