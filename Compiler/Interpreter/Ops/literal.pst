@import("ValueStackCapacityCheck.pst");
if (@ext_boolean("HAS_INCREMENT")) {
    valueStack[valueStackSize++] = literalTable[row[0]];
} else {
    valueStack[valueStackSize] = literalTable[row[0]];
    valueStackSize += 1;
}
