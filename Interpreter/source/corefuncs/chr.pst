@import("inline/1_arg.pst");

output = null;
if (arg1.type == Types.INTEGER) {
	int1 = (int) arg1.internalValue;
	if ((int1 >= 0) && (int1 < 256)) {
		output = buildCommonString(globals, Core.CharToString(Core.Chr(int1)));
	}
}

