/*
    arg 1: the string to parse.
*/
Value lib_json_parse(VmContext vm, Array<Value> args) {
    string raw = (string) args[0].internalValue;
    if (raw.Size() > 0) {
        Value output = Native._lib_json_parse(vm.globals, raw);
        if (output != null) {
            return output;
        }
    }
    return vm.globalNull;
}
